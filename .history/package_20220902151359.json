{
  "name": "american-eagle",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "node_modules/cucumber/bin/cucumber-js",
    "chrome": "cross-env BOWSER_ENW=chrome",
    "firefox": "cross-env BOWSER_ENW=firefox",
    "AE-scenario": "cross-env BOWSER_ENW=chrome cucumber-js --tags @AE ./src/features/ -f summary -f json:reporter/cucumber_report.json",
    "Minusrus-Scenario": "cross-env BOWSER_ENW=chrom cucumber-js --tags @minusrus  ./src/features/ -f summary -f json:reporter/cucumber_report.json || npm run reporter",
    "Combined-Scenarios": "cross-env BOWSER_ENW=chrome cucumber-js --tags \"@minusrus or @AE\" -f summary ./src/features/ -f summary -f json:reporter/cucumber_report.json",
    "build": "tsc",
    "clean-build": "npm run clean-dist && npm run build",
    "clean-dist": "rm -rf dist target/*",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint --fix **/*.ts",
    "reporter": "node reporter/index.js",
    "cucumber-reporter": "-f json:reporter/cucumber_report.json"
  },
  "dependencies": {
    "@cucumber/cucumber": "7.3.2",
    "@cucumber/pretty-formatter": "1.0.0-alpha.1",
    "@types/chai": "4.1.7",
    "@types/module-alias": "2.0.0",
    "@types/node": "17.0.23",
    "@types/selenium-webdriver": "4.0.18",
    "chai": "4.2.0",
    "chromedriver": "^105.0.0",
    "cross-spawn": "7.0.3",
    "geckodriver": "3.0.2",
    "module-alias": "2.1.0",
    "node-fetch": "2.6.7",
    "selenium-webdriver": "4.1.1",
    "ts-node": "8.1.0",
    "typedoc": "0.23.9",
    "typescript": "4.7.4",
    "winston": "3.8.0"
  },
  "devDependencies": {
    "@types/node-fetch": "2.6.2",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "cross-env": "7.0.3",
    "cucumber-html-reporter": "5.5.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.4",
    "eslint-plugin-n": "15.2.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-promise": "6.0.0"
  }
}
