    "@types/selenium-webdriver": "4.0.18",
    "chai": "4.2.0",
    "chromedriver": "102.0.0",
    "cross-spawn": "^7.0.3",
    "cucumber-html-reporter": "5.5.0",
    "geckodriver": "3.0.2",
    "module-alias": "2.1.0",
    "node-fetch": "2.6.7",
    "selenium-webdriver": "4.1.1",
    "ts-node": "8.1.0",
    "typescript": "4.3.5",
    "winston": "3.8.0"
  },
  "devDependencies": {
    "@types/node-fetch": "2.6.2",
